{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "gemini-proxy-panel",
	"main": "src/index.ts",
	"compatibility_date": "2025-03-27",
	"compatibility_flags": [
		"nodejs_compat"
	],
	"assets": {
		"binding": "ASSETS",
		"directory": "./public"
	},
	"observability": {
		"enabled": true
	},
	"kv_namespaces": [
		{ "binding": "GEMINI_KEYS_KV", "id": "123321"},
		{ "binding": "WORKER_CONFIG_KV", "id": "123321"}
	]
}